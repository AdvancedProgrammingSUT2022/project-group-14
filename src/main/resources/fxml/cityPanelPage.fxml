<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Text?>

<AnchorPane id="profileMenuScene" prefHeight="720" prefWidth="1280" stylesheets="@../css/style.css"
            xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="views.CityPanelPageController">
    <Button layoutX="50" layoutY="600" onMouseClicked="#backButtonClicked" text="Back"/>
    <Text fx:id="goldText" fill="white" layoutX="107.0" layoutY="51.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Text"/>
    <Text fx:id="foodText" fill="white" layoutX="107.0" layoutY="95.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Text"/>
    <Text fx:id="productionText" fill="white" layoutX="107.0" layoutY="139.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Text"/>
    <Text fx:id="populationText" fill="white" layoutX="107.0" layoutY="183.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Text"/>
    <Circle fx:id="goldCircle" layoutX="68.0" layoutY="44.0" radius="18.0"/>
    <Circle fx:id="foodCircle" layoutX="68.0" layoutY="88.0" radius="18.0"/>
    <Circle fx:id="productionCircle" layoutX="68.0" layoutY="132.0" radius="18.0"/>
    <Circle fx:id="populationCircle" layoutX="68.0" layoutY="176.0" radius="18.0"/>
    <Button layoutX="68.0" layoutY="264.0" onMouseClicked="#buyTileButtonClicked" text="Buy Tile"/>
    <Spinner layoutX="73.0" layoutY="316.0" prefHeight="25.0" prefWidth="60.0" fx:id="xTileSpinner"/>
    <Spinner fx:id="yTileSpinner" layoutX="153.0" layoutY="316.0" prefHeight="25.0" prefWidth="60.0"/>
    <Button layoutX="347.0" layoutY="264.0" onMouseClicked="#purchaseButtonClicked" text="Purchase"/>
    <ChoiceBox fx:id="unitsBox" layoutX="262.0" layoutY="316.0" prefWidth="150.0"/>
    <ChoiceBox fx:id="buildingsBox" layoutX="432.0" layoutY="316.0" prefWidth="150.0"/>
    <ChoiceBox fx:id="employedCitizensBox" layoutX="746.0" layoutY="176.0" prefWidth="150.0"/>
    <ChoiceBox fx:id="unemployedCitizensBox" layoutX="916.0" layoutY="176.0" prefWidth="150.0"/>
    <Button layoutX="746.0" layoutY="216.0" onMouseClicked="#unlockButtonClicked" text="Unlock"/>
    <Button layoutX="916.0" layoutY="263.0" onMouseClicked="#lockButtonClicked" text="Lock"/>
    <Spinner layoutX="921.0" layoutY="222.0" prefHeight="25.0" prefWidth="60.0" fx:id="xCitizenSpinner"/>
    <Spinner fx:id="yCitizenSpinner" layoutX="1001.0" layoutY="222.0" prefHeight="25.0" prefWidth="60.0"/>
    <Text fill="WHITE" layoutX="722.0" layoutY="110.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Current Production : "/>
    <Text fx:id="productionName" fill="WHITE" layoutX="917.0" layoutY="95.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Name : "/>
    <Text fx:id="productionRemainingCost" fill="WHITE" layoutX="917.0" layoutY="121.0" strokeType="OUTSIDE"
          strokeWidth="0.0" text="Remaining Cost : "/>
    <Text fx:id="infoText" fill="white" layoutX="218.0" layoutY="456.0"/>
    <Text fx:id="citizensText" fill="WHITE" layoutX="735.0" layoutY="365.0" text="Text"/>

</AnchorPane>
