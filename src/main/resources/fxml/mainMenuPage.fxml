<?import javafx.scene.text.Text?>
<AnchorPane id="mainMenuScene" prefHeight="720" prefWidth="1280" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.MainMenuController">

      <Text layoutX="100" layoutY="100" styleClass="sceneTitle" text="Main Menu" />

      <Button layoutX="145.0" layoutY="240.0" text="start game" onMouseClicked="#startGame"/>
      <Button layoutX="145.0" layoutY="300.0" text="profile" onMouseClicked="#goToProfileMenu"/>
      <Button layoutX="145.0" layoutY="360.0" text="scoreboard" onMouseClicked="#goToScoreboard"/>
      <Button layoutX="145.0" layoutY="420.0" text="chatroom" onMouseClicked="#goToChatroom"/>
      <Button layoutX="145.0" layoutY="480.0" text="logout" onMouseClicked="#logout"/>

</AnchorPane>
