<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<ScrollPane id="technologyTreeScene" fx:id="scrollPane" hbarPolicy="ALWAYS" prefHeight="680.0" prefWidth="1265.0" stylesheets="@../css/style.css" vbarPolicy="ALWAYS" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.views.TechnologyTreeController">
    <AnchorPane fx:id="anchorPane" prefHeight="720.0" prefWidth="1280.0">
        <HBox fx:id="hBox" prefHeight="720.0" prefWidth="1280.0" />
        <Button layoutX="50" layoutY="600" onMouseClicked="#backButtonClicked" text="Back" />
        <TextField fx:id="searchField" layoutX="20.0" layoutY="20.0" promptText="Enter technology name" />
        <Button layoutX="20.0" layoutY="60.0" mnemonicParsing="false" onMouseClicked="#searchButtonClicked" style="-fx-pref-width: 100;" text="Search" />
      <Text fill="CADETBLUE" layoutX="350.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Researched" />
      <Text fill="GOLD" layoutX="470.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Researching" />
      <Text fill="RED" layoutX="595.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Not started" />
    </AnchorPane>
</ScrollPane>
